Resources
| where type =~ 'microsoft.compute/disks'
| where name has 'IdentityDisk'
| extend diskSizeGB = tostring(properties.diskSizeGb), managedBy = tostring(properties.ownerId)
| mv-expand tagPair = bag_unpack(tags)
| project diskName = name, diskRG = resourceGroup, location, diskSizeGB, managedBy,
          tagKey = tostring(tagPair[0]), tagValue = tostring(tagPair[1])

